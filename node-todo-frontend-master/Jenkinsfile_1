//Jenkins file only to tutorial example

pipeline {
  agent any
    
  tools {nodejs "node"}
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/muneeb126/npm.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        sh 'npm clean-install'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm test'
      }
    } 
    }
    post {
        always {
            deleteDir()
        }
        }
}
