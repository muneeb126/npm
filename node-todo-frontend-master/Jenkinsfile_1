//Jenkins file only to tutorial example

pipeline {
  agent any
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/muneeb126/npm.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        sh 'npm clean-install'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm test'
      }
    } 
    }
}
